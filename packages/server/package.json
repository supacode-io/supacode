{
    "name": "@supacode/server",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "PORT=4901 nodemon index.js | pino-pretty",
        "dev:debug": "PORT=4901 ROARR_LOG=true nodemon index.js | roarr pretty-print ",
        "start": "PORT=4901 node index.js",
        "db:push": "dotenv -e ../../.env -- prisma db push --skip-generate",
        "db:reset": "dotenv -e ../../.env -- prisma db push --force-reset --skip-generate",
        "db:migrate:old:debug": "ROARR_LOG=true node ./db/migrate-old/index.js | roarr pretty-print",
        "db:migrate:old": "node ./db/migrate-old/index.js",
        "db:migrate:webflow:debug": "ROARR_LOG=true node ./db/webflow-migrations/index.js | roarr pretty-print",
        "db:migrate:webflow": "node ./db/webflow-migrations/index.js"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.282.0",
        "@aws-sdk/s3-request-presigner": "^3.282.0",
        "@fastify/cookie": "^8.3.0",
        "@fastify/cors": "^8.2.0",
        "@fastify/rate-limit": "^7.6.0",
        "@fastify/session": "^10.1.1",
        "@roarr/cli": "^5.10.4",
        "@supacode/eslint-config": "workspace:*",
        "bcrypt": "^5.1.0",
        "connect-redis": "^6.1.3",
        "date-fns": "^2.29.3",
        "dotenv-cli": "^6.0.0",
        "fastify": "^4.13.0",
        "ioredis": "^5.3.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mime": "^3.0.0",
        "postgres": "^3.3.3",
        "qs": "^6.11.0",
        "slonik": "^33.1.0",
        "slonik-interceptor-query-logging": "^1.4.7",
        "slonik-sql-tag-raw": "^1.1.4",
        "uid-safe": "^2.1.5"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "axios": "^1.3.4",
        "cli-progress": "^3.12.0",
        "dotenv": "^16.0.3",
        "dotenv-expand": "^9.0.0",
        "nodemon": "^2.0.21",
        "pino-pretty": "^9.4.0",
        "prisma": "^4.11.0",
        "roarr": "^7.14.3",
        "uuid": "^9.0.0"
    }
}